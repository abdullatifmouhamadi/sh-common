#!/usr/bin/python
class SupplierInfo:
    def __init__(self):
        self._name = None
        self._company_id = None
        self._currency_id = None
        self._delay = None
        self._min_qty = None
        self._price = None



    def get_name(self):
        return self._name
    def get_company_id(self):
        return self._company_id
    def get_currency_id(self):
        return self._currency_id
    def get_delay(self):
        return self._delay
    def get_min_qty(self):
        return self._min_qty
    def get_price(self):
        return self._price